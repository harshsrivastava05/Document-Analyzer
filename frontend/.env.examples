# NextAuth Configuration
AUTH_SECRET="your-super-secret-key-here"
AUTH_GOOGLE_ID="your-google-oauth-client-id"
AUTH_GOOGLE_SECRET="your-google-oauth-client-secret"
NEXTAUTH_URL="http://localhost:3000"

# Database - IMPORTANT: Use PostgreSQL
DATABASE_URL="postgresql://username:password@ep-your-endpoint.region.aws.neon.tech/neondb?sslmode=require"

# For local PostgreSQL (alternative):
# DATABASE_URL="postgresql://postgres:your-password@localhost:5432/doc_analyzer"

# Google Cloud Storage Configuration
GCS_BUCKET_NAME="your-bucket-name"
GCS_PROJECT_ID="your-gcp-project-id"

# For local development - use service account key file path
GOOGLE_APPLICATION_CREDENTIALS="./path/to/service-account-key.json"

# For production deployment - use base64 encoded key
# GCS_SERVICE_ACCOUNT_KEY_BASE64="your-base64-encoded-service-account-key"

# JWT Secret - IMPORTANT: This MUST match the backend JWT_SECRET exactly!
JWT_SECRET="your-super-secret-jwt-key-change-this-in-production"

# Backend API URL
NEXT_PUBLIC_BACKEND_URL="http://127.0.0.1:8000"